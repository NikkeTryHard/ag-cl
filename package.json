{
  "name": "ag-cl",
  "version": "1.3.0",
  "description": "Proxy server to use Antigravity's Claude models with Claude Code CLI",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "ag-cl": "bin/cli.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "tui": "tsx src/cli/index.ts tui",
    "start": "tsx src/cli/index.ts start",
    "start:prod": "node dist/cli/index.js start",
    "dev": "tsx --watch src/cli/index.ts start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "init": "tsx src/cli/index.ts init",
    "accounts": "tsx src/cli/index.ts accounts",
    "accounts:add": "tsx src/cli/index.ts accounts add",
    "accounts:list": "tsx src/cli/index.ts accounts list",
    "accounts:remove": "tsx src/cli/index.ts accounts remove",
    "accounts:verify": "tsx src/cli/index.ts accounts verify",
    "accounts:clear": "tsx src/cli/index.ts accounts clear",
    "trigger-reset": "tsx src/cli/index.ts trigger-reset",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:bench": "vitest bench",
    "test:integration": "node tests/integration/run-all.cjs",
    "test:load": "vitest run tests/load/",
    "test:signatures": "node tests/integration/test-thinking-signatures.cjs",
    "test:multiturn": "node tests/integration/test-multiturn-thinking-tools.cjs",
    "test:streaming": "node tests/integration/test-multiturn-thinking-tools-streaming.cjs",
    "test:interleaved": "node tests/integration/test-interleaved-thinking.cjs",
    "test:images": "node tests/integration/test-images.cjs",
    "test:caching": "node tests/integration/test-caching-streaming.cjs",
    "test:crossmodel": "node tests/integration/test-cross-model-thinking.cjs",
    "test:oauth": "node tests/integration/test-oauth-no-browser.cjs",
    "prepublishOnly": "npm run build",
    "upstream:fetch": "git fetch upstream",
    "upstream:log": "git fetch upstream && git log upstream-synced..upstream/main --oneline",
    "upstream:diff": "git fetch upstream && git diff upstream-synced..upstream/main --stat",
    "upstream:diff-full": "git fetch upstream && git diff upstream-synced..upstream/main",
    "upstream:mark": "git tag -f upstream-synced upstream/main && echo 'Bookmark updated to current upstream/main'",
    "upstream:status": "git fetch upstream && echo '=== Upstream Status ===' && echo \"Bookmarked at: $(git rev-parse --short upstream-synced)\" && echo \"Upstream HEAD: $(git rev-parse --short upstream/main)\" && echo \"New commits: $(git rev-list upstream-synced..upstream/main --count)\""
  },
  "keywords": [
    "claude",
    "anthropic",
    "antigravity",
    "proxy",
    "vertex-ai"
  ],
  "author": "NikkeTryHard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikkeTryHard/ag-cl.git"
  },
  "homepage": "https://github.com/NikkeTryHard/ag-cl#readme",
  "bugs": {
    "url": "https://github.com/NikkeTryHard/ag-cl/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "ag-cl": "^1.2.0",
    "better-sqlite3": "^12.5.0",
    "boxen": "^8.0.1",
    "cli-table3": "^0.6.5",
    "clipboardy": "^5.0.2",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fuzzysort": "^3.1.0",
    "ink": "^5.2.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "open": "^11.0.0",
    "ora": "^9.0.0",
    "picocolors": "^1.1.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/react": "^14.3.1",
    "@types/autocannon": "^7.12.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "autocannon": "^8.0.0",
    "eslint": "^9.39.2",
    "expect-type": "^1.3.0",
    "fast-check": "^4.5.3",
    "ink-testing-library": "^4.0.0",
    "jsdom": "^27.4.0",
    "nock": "^14.0.10",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  }
}
